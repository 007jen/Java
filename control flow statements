#loops 

import java.util.Scanner;

class loops
{
    public  static void  main(String args[])
    {
        int a=1;

        while (a<=10)
        {
            System.out.println(a);
            a++;
        }
    System.out.println();
        for (a=1;a<=10;a++)
        {
            System.out.println(a);

        }
System.out.println();
        do {
            System.out.println(a);
        }while (a<=10);
    }


}
